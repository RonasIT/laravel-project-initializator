{
    "name": "ronasit/laravel-project-initializator",
    "description": "Provided tools for project initialization.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RonasIT\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RonasIT\\Support\\Tests\\": "tests/",
            "RonasIT\\Support\\Tests\\Support\\": "tests/Support/"
        }
    },
    "minimum-stability": "alpha",
    "require": {
        "php": ">=8.3",
        "laravel/framework": "^11.20",
        "php-mock/php-mock-phpunit": "^2.10",
        "ext-json": "*",
        "ronasit/laravel-helpers": "^3.0.0-beta"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23.1",
        "mockery/mockery": "^1.6.12",
        "phpunit/phpunit": "^10.5.30",
        "php-coveralls/php-coveralls": "^2.7",
        "orchestra/testbench": "^9.3",
        "friendsofphp/php-cs-fixer": "^3.62",
        "brainmaestro/composer-git-hooks": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RonasIT\\Support\\ProjectInitializatorServiceProvider"
            ]
        }
    }
}
