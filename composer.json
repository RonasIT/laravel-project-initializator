{
    "name": "ronas/laravel-project-initializator",
    "description": "Create new Laravel Application",
    "keywords": [
        "ronas",
        "laravel-project-initializator"
    ],
    "homepage": "https://github.com/ronas/laravel-project-initializator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mher Shahinyan",
            "email": "mshahinyan@ronasit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ronas\\LaravelProjectInitializator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ronas\\LaravelProjectInitializator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ronas\\LaravelProjectInitializator\\LaravelProjectInitializatorServiceProvider"
            ],
            "aliases": {
                "LaravelProjectInitializator": "Ronas\\LaravelProjectInitializator\\LaravelProjectInitializatorFacade"
            }
        }
    }
}
